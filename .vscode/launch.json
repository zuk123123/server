{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run AuthServer (uvicorn)",
      "type": "python",
      "request": "launch",
      "module": "uvicorn",
      "args": [
        "app.server:app",
        "--host", "0.0.0.0",
        "--port", "8080"
      ],
      "envFile": "${workspaceFolder}/.env",
      "jinja": true,
      "console": "integratedTerminal"
    }
  ]
}
